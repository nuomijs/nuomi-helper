{
	"name": "nuomi-helper",
	"publisher": "yinjiazeng",
	"displayName": "nuomi-helper",
	"description": "nuomi helper for vscode",
	"version": "0.0.1",
	"repository": "https://github.com/nuomijs/nuomi-helper",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createNuomiFolder"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.createNuomiFolder",
				"title": "新建Nuomi文件夹"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.createNuomiFolder",
					"group": "navigation",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"fs-extra": "^8.1.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
